{
  "name": "vrestihnat/meta-canonical-control",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "Nette control for managing meta data and canonical link in HTML header.",
  "keywords": ["meta", "header", "component", "nette", "canonical", "seo"],
  "support": {
    "email": "bfj@bflm.eu",
    "issues": "https://github.com/vrestihnat/title-control/issues"
  },
  "authors": [
    {
      "name": "Petr Morávek",
      "email": "petr@pada.cz"
    },
    {
      "name": "Jan Záruba",
      "email": "bfj@bflm.eu"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "nette/application": "^3.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Vrestihnat\\MetaControl\\": "src/MetaControl/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VrestihnatTests\\": "tests/"
    }
  },
  "config": {
    "platform-check": false
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.69",
    "phpstan/phpstan-nette": "^0.12.14",
    "spaze/phpstan-disallowed-calls": "^1.1",
    "phpunit/php-code-coverage": "^9.2"
  },
  "scripts": {
        "phpstan": "phpstan analyse src/ -c phpstan.neon.dist --level=4 --no-progress -vvv"
    }
}
